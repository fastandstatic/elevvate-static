<script>
function handleSubmit(event) {
    event.preventDefault();
    const data = new FormData(event.target);
    const value = Object.fromEntries(data.entries());
    const elements = event.target.elements;
    console.log("1")
    console.log(value);

    var obj = {};
    for (var i = 0; i < elements.length; i++) {
        var item = elements.item(i);
        obj[item.name] = item.value;
    }

    console.log(obj);

    // Cierra el popup de Elementor
    closedpopup();

    fetch("https://hook.us1.make.com/h15hml1s6j7guic5c7ktf4iffrob2v9y", {
        body: JSON.stringify(value),
        headers: {
            Accept: 'application/json',
            'Content-Type': 'application/json',
        },
        method: "post",
    })
        .then((value) => {
            console.log('todo ok');
            evento();

            Swal.fire({
    title: "Use the following link to download the PDF",
    icon: 'success',
    confirmButtonColor: "#000000",
    confirmButtonText: "Download PDF", // texto del bot√≥n
    customClass: {
        confirmButton: "mi-boton-descarga" // "mi-boton-descarga"
    }
}).then((result) => {
    if (result.isConfirmed) {
        // Abre el PDF en una nueva pesta√±a
        window.open("https://elevvate.co/Pricing-elevvate.pdf", "_blank");
    }
});

        })
        .catch((error) => {
            console.error('Error:', error);
        });
}

function closedpopup() {
    const popup = document.querySelector('#elementor-popup-modal-45335');
    popup.style.display = 'none';
}

function evento() {
    var evento = "form-elevvate-pdf";
    fbq("trackCustom", evento),
        gtag("event", evento,
            {
                event_category: "Form",
                event_label: evento
            });
}

function utm_llenar() {
    const valores = window.location.search;
    const urlParams = new URLSearchParams(valores);
    var term = urlParams.get('utm_term');
    var medium = urlParams.get('utm_medium');
    var campaign = urlParams.get('utm_campaign');
    var source = urlParams.get('utm_source');
    var content = urlParams.get('utm_content');
    document.getElementById("form-field-utm_term").value = term;
    document.getElementById("form-field-utm_medium").value = medium;
    document.getElementById("form-field-utm_campaign").value = campaign;
    document.getElementById("form-field-utm_source").value = source;
    document.getElementById("form-field-utm_content").value = content;
}

function addEvent() {
     // Se coloca ID del formulari
     
        // sm√°s de un formulario en la web se cambia la variable form agregando un numero al final
    utm_llenar();
    const form = document.querySelector('#form_pdf');
    console.log("form", form);
    if (!form) {
        "No se encontr√≥ formulario con id";
    }
    form.addEventListener('submit', handleSubmit);
}

window.onload = addEvent();
</script>		</div>
